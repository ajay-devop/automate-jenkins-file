pipeline { 
         agent {
	        label "aj-slave"
	 }
	  stages {
	         stage (Pull the code) {
		       steps {
		              git branch: 'main', url: 'https://github.com/ajay-devop/automate-jenkins-file.git'
		       }
		 }
		 stage (package Building) {
		       steps {
		              sh 'sudo mvn clean package'
		       }
		 }
	  }
}

